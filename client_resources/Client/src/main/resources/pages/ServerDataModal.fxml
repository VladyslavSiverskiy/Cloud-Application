<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="230.0" prefWidth="303.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ServerDataModalController">

        <children>
            <TextField fx:id="serverIP" layoutX="77.0" layoutY="61.0" promptText="127.0.0.1" text="localhost" />
            <Label layoutX="107.0" layoutY="38.0" text="Enter ip of server" />
            <Button layoutX="122.0" layoutY="171.0" mnemonicParsing="false" onAction="#connectToServer" text="Connect" />
            <Label fx:id="warningLabel" layoutX="14.0" layoutY="86.0" />
      <TextField fx:id="serverPort" layoutX="78.0" layoutY="132.0" promptText="127.0.0.1" text="23333" />
      <Label layoutX="104.0" layoutY="107.0" text="Enter port number" />
        </children>

</AnchorPane>
